<section class="gko-ptrnlab-colours">
	<header class="gko-ptrnlab-doc-header">
		<h1 class="gko-ptrnlab-doc-header__heading">Heading elements</h1>
	</header>

	{{#each typography.headings}}
		{{{heading}}}
	{{/each}}
</section>

<section class="gko-ptrnlab-colours">
	<header class="gko-ptrnlab-doc-header">
		<h1 class="gko-ptrnlab-doc-header__heading">Intro text</h1>
	</header>

	{{{typography.intro}}}
</section>

<section class="gko-ptrnlab-colours">
	<header class="gko-ptrnlab-doc-header">
		<h1 class="gko-ptrnlab-doc-header__heading">Paragraph text</h1>
	</header>

	{{{typography.paragraph}}}
</section>

<section class="gko-ptrnlab-colours">
	<header class="gko-ptrnlab-doc-header">
		<h1 class="gko-ptrnlab-doc-header__heading">Unordered list</h1>
	</header>

	<ul>
		{{#each typography.unordered-list}}
			<li>
				{{item}}
				{{#if child-unordered-list}}
					<ul>
						{{#each child-unordered-list}}
							<li>{{item}}</li>
						{{/each}}
					</ul>
				{{/if}}
			</li>
		{{/each}}
	</ul>
</section>

<section class="gko-ptrnlab-colours">
	<header class="gko-ptrnlab-doc-header">
		<h1 class="gko-ptrnlab-doc-header__heading">Ordered list</h1>
	</header>

	<ol>
		{{#each typography.ordered-list}}
			<li>
				{{item}}
				{{#if child-ordered-list}}
					<ol>
						{{#each child-ordered-list}}
							<li>{{item}}</li>
						{{/each}}
					</ol>
				{{/if}}
			</li>
		{{/each}}
	</ol>
</section>

<section class="gko-ptrnlab-colours">
	<header class="gko-ptrnlab-doc-header">
		<h1 class="gko-ptrnlab-doc-header__heading">Blockquote</h1>
	</header>

	{{> blockquote blockquote }}
</section>
